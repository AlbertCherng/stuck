<% include ./partials/_header %>

<div class="jumbotron">
    <div class="col-xs-6">
      <h2 class="sub-header">Choices</h2>
      <div class="table-responsive">
        <table class="table table-striped">
        <thead>
          <tr>
          </tr>
        </thead>
        <tbody>
          <% for (title in titles) { %>
          <tr>
            <td><%= titles[title]%></td>
          </tr>
          <% } %>
        </tbody>
      </table>
      </div>
    </div>

<div class="col-xs-6">
  <h2 class="sub-header">Combined Rankings</h2>
    <div class="table-responsive">
      <table class="table table-striped">
    <thead>
      <tr>
      </tr>
    </thead>
    <tbody>
    <% for(item in answer) { %>
      <tr>
        <td><%= answer[item] %></td>
      </tr>
    <% } %>
    </tbody>
  </table>
</div>
</div>


  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <button type="button" class="btn btn-primary btn-lg"><span class="glyphicon glyphicon-tasks" aria-hidden="true"></span> Results</button>
      <p>
        <form id="adminLink">
          <input type="text" class="form-control" name="adminlink" value="http://localhost:8080/polls/<%= id%>/results">
        </form>
      </p>
      <button type="button" class="btn btn-info btn-lg"><span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> Share </button>
      <p>
        <form id="adminLink">
          <input type="text" class="form-control" name="adminlink" value="http://localhost:8080/rank/<%= id%>">
        </form>
      </p>
    </div>
  </div>
</div>

<% include ./partials/_footer %>
